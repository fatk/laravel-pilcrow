{
  "name": "fatk/pilcrow",
  "description": "A collection of WordPress utilities and helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Karl Fathi",
      "email": "karl@fatk.dev",
      "homepage": "https://karlfathi.com"
    }
  ],
  "homepage": "https://github.com/fatk/laravel-pilcrow",
  "keywords": [
    "Laravel",
    "WordPress",
    "Helper",
    "Import"
  ],
  "require": {
    "illuminate/support": "~10"
  },
  "autoload": {
    "psr-4": {
      "Fatk\\Pilcrow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fatk\\Pilcrow\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fatk\\Pilcrow\\PilcrowServiceProvider"
      ],
      "aliases": {
        "Pilcrow": "Fatk\\Pilcrow\\Facades\\Pilcrow",
        "PostType": "Fatk\\Pilcrow\\Facades\\PostType"
      }
    }
  }
}
