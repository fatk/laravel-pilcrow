{
  "name": "fatk/pilcrow",
  "description": "A collection of WordPress utilities and helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Karl Fathi",
      "email": "karl@fatk.dev",
      "homepage": "https://karlfathi.com"
    }
  ],
  "homepage": "https://github.com/fatk/laravel-pilcrow",
  "keywords": [
    "Laravel",
    "WordPress",
    "Helper",
    "Import"
  ],
  "require": {
    "php": "^8.1",
    "roots/acorn": "^4.3",
    "laravel/prompts": "^0.1",
    "maatwebsite/excel": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Fatk\\Pilcrow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fatk\\Pilcrow\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fatk\\Pilcrow\\PilcrowServiceProvider"
      ],
      "aliases": {
        "Pilcrow": "Fatk\\Pilcrow\\Facades\\Pilcrow"
      }
    }
  }
}
